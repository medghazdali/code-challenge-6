{
  "type": "object",
  "properties": {
    "accessToken": {
      "type": "string",
      "description": "JWT access token for API authentication",
      "example": "eyJraWQiOiJ..."
    },
    "idToken": {
      "type": "string",
      "description": "JWT ID token containing user identity",
      "example": "eyJraWQiOiJ..."
    },
    "refreshToken": {
      "type": "string",
      "description": "Refresh token for obtaining new access tokens",
      "example": "eyJjdHkiOiJ..."
    },
    "expiresIn": {
      "type": "integer",
      "description": "Token expiration time in seconds",
      "example": 3600
    }
  },
  "required": ["accessToken", "idToken", "refreshToken", "expiresIn"]
}

