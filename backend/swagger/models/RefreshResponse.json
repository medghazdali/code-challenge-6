{
  "type": "object",
  "properties": {
    "accessToken": {
      "type": "string",
      "description": "New JWT access token",
      "example": "eyJraWQiOiJ..."
    },
    "idToken": {
      "type": "string",
      "description": "New JWT ID token",
      "example": "eyJraWQiOiJ..."
    },
    "expiresIn": {
      "type": "integer",
      "description": "Token expiration time in seconds",
      "example": 3600
    }
  },
  "required": ["accessToken", "idToken", "expiresIn"]
}

